    'covariance-functions.R'
    'evidence.R'
    'ppca-initialization.R'
    'prior.R'
    'statistical-quantities.R'
    'stpca.R'
Imports: functional, MASS, fields, maxLik, Matrix, numDeriv
ByteCompile: true
